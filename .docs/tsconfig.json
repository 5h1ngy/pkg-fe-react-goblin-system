{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "../node_modules/.tmp/tsconfig.docs.tsbuildinfo",
    "baseUrl": ".",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "paths": {
      "pkg-fe-react-goblin-system": ["../packages/index.ts"],
      "pkg-fe-react-goblin-system/*": ["../packages/*"],
      "@site/*": ["./*"]
    },
    "types": ["@docusaurus/module-type-aliases"]
  },
  "include": ["src", "../docs", "static", "./*.ts", "../packages/foundations/styled.d.ts"],
  "exclude": [".docusaurus", "build", "node_modules"]
}
