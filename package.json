  "engines": {
    "node": ">=20.18.0",
    "yarn": ">=4.5.1"
  },
  "packageManager": "yarn@4.5.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "import": "./dist/*",
      "require": "./dist/*",
      "types": "./dist/types/*"
    }
  },
  "typesVersions": {
    "*": {
      "react-goblin-system": [
        "dist/types/index.d.ts"
      ],
      "react-goblin-system/*": [
        "dist/types/*"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "alias": {
    "@": "./src"
  },
  "scripts": {
    "start:storybook": "storybook dev -p 6006 --no-open",
    "start:docs": "docusaurus start",
    "test": "jest --passWithNoTests",
    "build:storybook": "storybook build --output-dir build/storybook/",
    "build:docs": "docusaurus build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:types": "tsc --project tsconfig.types.json",
    "clean": "rm -rfdv node_modules dist"
  },
  "dependencies": {
    "axios": "^1.7.9",
