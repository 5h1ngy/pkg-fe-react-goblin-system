  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start:storybook": "storybook dev",
    "start:docs": "cd docs && npm run start",
    "build:storybook": "storybook build --output-dir build-storybook",
    "build:docs": "cd docs && npm run build && rm -rf ../build-docusaurus && mv build ../build-docusaurus",
    "build:lib": "vite build --config vite.config.lib.ts --mode prod && tsc --project tsconfig.types.json && npm pack && mkdir -p build-lib/ && mv *.tgz build-lib/",
    "reset": "rm -rf node_modules dist build-docusaurus build-storybook build-lib docs/node_modules docs/.docusaurus",
    "clean": "rm -rf dist build-docusaurus build-storybook build-lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "npm run build:lib",
    "docusaurus": "cd docs && docusaurus"
  },
  "dependencies": {
    "axios": "^1.8.2",
